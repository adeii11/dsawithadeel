class Solution {
    public boolean validPalindrome(String s) {
        int st=0;
        int e=s.length()-1;
        while(st<e){
            if(s.charAt(st)==s.charAt(e)){
                st++;
                e--;
            }else if(s.charAt(st)!=s.charAt(e)){
                return false;
            }
        }
        return true;
    }
}
